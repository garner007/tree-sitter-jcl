{
  "name": "tree-sitter-jcl",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for JCL (Job Control Language)",
  "main": "bindings/node",
  "keywords": [
    "tree-sitter",
    "jcl",
    "mainframe",
    "zos",
    "parser"
  ],
  "scripts": {
    "test": "mocha test/**/*.test.js",
    "test:watch": "mocha test/**/*.test.js --watch",
    "build": "tree-sitter generate && node-gyp build",
    "generate": "tree-sitter generate",
    "parse": "tree-sitter parse",
    "test:corpus": "tree-sitter test",
    "lint": "eslint grammar.js test/**/*.js",
    "coverage": "nyc npm test"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.17.0",
    "tree-sitter": "^0.25.0"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "eslint": "^8.50.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "tree-sitter-cli": "^0.20.8"
  },
  "tree-sitter": [
    {
      "scope": "source.jcl",
      "file-types": [
        "jcl",
        "job",
        "proc",
        "prc"
      ]
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/tree-sitter-jcl.git"
  }
}
